{
  "cli": {
    "version": ">= 0.44.1",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "22.17.1",
      "pnpm": "10.13.1"
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": { "simulator": true },
      "node": "22.17.1",
      "pnpm": "10.13.1"
    },
    "preview": {
      "channel": "preview",
      "env": { "APP_VARIANT": "preview" },
      "distribution": "internal",
      "node": "22.17.1",
      "pnpm": "10.13.1"
    },
    "staging": {
      "env": { "APP_VARIANT": "staging" },
      "distribution": "internal",
      "node": "22.17.1",
      "pnpm": "10.13.1"
    },
    "production": {
      "env": { "APP_VARIANT": "production" },
      "distribution": "store",
      "autoIncrement": true,
      "node": "22.17.1",
      "pnpm": "10.13.1"
    }
  },
  "submit": {
    "production": { "ios": { "ascAppId": "6476064177" } }
  }
}
