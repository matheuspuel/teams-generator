{
  "name": "teams-generator",
  "private": true,
  "version": "1.3.5",
  "scripts": {
    "start": "cross-env APP_VARIANT=development expo start",
    "android": "cross-env APP_VARIANT=development expo run:android",
    "ios": "cross-env APP_VARIANT=development expo run:ios",
    "web": "cross-env APP_VARIANT=development expo start --web",
    "experiment": "tsx watch scratchpad/experiment.ts",
    "lint": "eslint . --config .eslintrc.production.js",
    "lint:fix": "eslint . --fix --config .eslintrc.production.js",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "test:ci": "vitest --run",
    "coverage": "vitest --run --coverage related",
    "coverage:all": "vitest --run --coverage",
    "build:development:android": "eas build --platform android --profile development",
    "build:development:ios": "eas build --platform ios --profile development",
    "build:development:ios:simulator": "eas build --platform ios --profile development-simulator",
    "build:preview": "eas build --platform android --profile preview",
    "build:staging": "eas build --platform android --profile staging",
    "build:production": "eas build --platform android --profile production --auto-submit",
    "build:production:ios": "eas build --platform ios --profile production --auto-submit",
    "update:preview": "cross-env APP_VARIANT=preview eas update --auto --platform android --channel preview",
    "update:staging": "cross-env APP_VARIANT=staging eas update --auto --platform android --channel staging",
    "update:production": "cross-env APP_VARIANT=production eas update --auto --platform android --channel production"
  },
  "dependencies": {
    "@effect/schema": "^0.70.1",
    "@effect/typeclass": "^0.24.0",
    "@expo/vector-icons": "14.0.2",
    "@fp-ts/optic": "^0.24.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@react-native-async-storage/async-storage": "~1.23.1",
    "@react-navigation/elements": "^1.3.29",
    "@react-navigation/native": "^6.1.16",
    "cross-env": "^7.0.3",
    "effect": "3.6.0",
    "expo": "^51.0.24",
    "expo-application": "~5.9.1",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.21",
    "expo-device": "~6.0.2",
    "expo-document-picker": "~12.0.2",
    "expo-file-system": "~17.0.1",
    "expo-linking": "~6.3.1",
    "expo-localization": "~15.0.3",
    "expo-sharing": "~12.0.1",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.21",
    "fast-text-encoding": "^1.0.6",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@changesets/cli": "^2.27.7",
    "@cspell/dict-pt-br": "^2.3.0",
    "@expo/config-types": "51.0.2",
    "@types/benchmark": "^2.1.5",
    "@types/lodash.throttle": "^4.1.9",
    "@types/react": "~18.2.79",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^2.0.5",
    "babel-plugin-module-resolver": "^5.0.2",
    "benchmark": "^2.1.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-functional": "^6.6.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "fast-check": "^3.20.0",
    "prettier": "^3.3.3",
    "react-test-renderer": "~18.2.0",
    "rimraf": "^6.0.1",
    "tsutils": "^3.21.0",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vitest": "^2.0.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/preset-env",
        "@types/node",
        "expo-modules-autolinking"
      ]
    }
  }
}
