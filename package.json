    "fast-check": "^3.8.0",
    "jest": "^29.5.0",
    "jest-expo": "^48.0.2",
    "prettier": "^2.8.7",
    "react-devtools": "^4.27.4",
    "react-test-renderer": "18.2.0",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsutils": "^3.21.0",
    "typescript": "^5.0.4"
