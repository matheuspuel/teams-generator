{
  "name": "sorteio-times",
  "private": true,
  "version": "1.0.9",
  "scripts": {
    "experiment": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/experiment.ts",
    "start": "SET APP_VARIANT=development&& expo start",
    "android": "SET APP_VARIANT=development&& expo run:android",
    "ios": "SET APP_VARIANT=development&& expo run:ios",
    "web": "SET APP_VARIANT=development&& expo start --web",
    "lint": "eslint .",
    "eas-build-pre-install": "npm i -g pnpm@8.6.12",
    "build:development": "eas build --platform android --profile development",
    "build:preview": "eas build --platform android --profile preview",
    "build:staging": "eas build --platform android --profile staging",
    "build:production": "eas build --platform android --profile production --auto-submit",
    "update:preview": "SET APP_VARIANT=preview&& eas update --auto --platform android --channel preview",
    "update:staging": "SET APP_VARIANT=staging&& eas update --auto --platform android --channel staging",
    "update:production": "SET APP_VARIANT=production&& eas update --auto --platform android --channel production"
  },
  "dependencies": {
    "@effect/data": "0.17.1",
    "@effect/io": "0.38.1",
    "@effect/match": "0.32.0",
    "@effect/schema": "0.33.1",
    "@expo/vector-icons": "13.0.0",
    "@fp-ts/optic": "0.9.0",
    "@react-native-async-storage/async-storage": "~1.18.2",
    "@react-navigation/elements": "^1.3.18",
    "@react-navigation/native": "^6.1.7",
    "cuid": "^2.1.8",
    "expo": "^49.0.7",
    "expo-application": "~5.3.0",
    "expo-constants": "~14.4.2",
    "expo-device": "^5.4.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-updates": "~0.18.11",
    "fp-ts": "^2.16.1",
    "lodash.throttle": "^4.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.3",
    "react-native-gesture-handler": "~2.12.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@cspell/dict-pt-br": "^2.2.0",
    "@expo/config-types": "49.0.0",
    "@types/benchmark": "^2.1.2",
    "@types/jest": "^29.5.3",
    "@types/lodash.throttle": "^4.1.7",
    "@types/react": "~18.2.20",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "benchmark": "^2.1.4",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "fast-check": "^3.12.0",
    "jest": "^29.6.2",
    "jest-expo": "^49.0.0",
    "prettier": "^3.0.2",
    "react-test-renderer": "18.2.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsutils": "^3.21.0",
    "typescript": "^5.1.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/preset-env",
        "@types/node",
        "expo-modules-autolinking"
      ]
    }
  }
}
