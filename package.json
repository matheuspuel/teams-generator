{
  "name": "sorteio-times",
  "private": true,
  "version": "1.2.3",
  "scripts": {
    "experiment": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/experiment.ts",
    "start": "SET APP_VARIANT=development&& expo start",
    "android": "SET APP_VARIANT=development&& expo run:android",
    "ios": "SET APP_VARIANT=development&& expo run:ios",
    "web": "SET APP_VARIANT=development&& expo start --web",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "eas-build-pre-install": "npm i -g pnpm@8.10.0",
    "build:development": "eas build --platform android --profile development",
    "build:preview": "eas build --platform android --profile preview",
    "build:staging": "eas build --platform android --profile staging",
    "build:production": "eas build --platform android --profile production --auto-submit",
    "update:preview": "eas update --auto --platform android --channel preview",
    "update:staging": "eas update --auto --platform android --channel staging",
    "update:production": "eas update --auto --platform android --channel production"
  },
  "dependencies": {
    "@effect/match": "^0.40.0",
    "@effect/schema": "^0.42.0",
    "@expo/vector-icons": "13.0.0",
    "@fp-ts/optic": "^0.9.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@react-native-async-storage/async-storage": "~1.18.2",
    "@react-navigation/elements": "^1.3.19",
    "@react-navigation/native": "^6.1.8",
    "effect": "2.0.0-next.47",
    "expo": "^49.0.16",
    "expo-application": "~5.3.0",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.8",
    "expo-device": "~5.4.0",
    "expo-document-picker": "~11.5.4",
    "expo-file-system": "~15.4.3",
    "expo-linking": "~5.0.2",
    "expo-sharing": "~11.5.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-updates": "~0.18.16",
    "fast-text-encoding": "^1.0.6",
    "fp-ts": "^2.16.1",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@cspell/dict-pt-br": "^2.2.1",
    "@expo/config-types": "49.0.0",
    "@types/benchmark": "^2.1.4",
    "@types/jest": "^29.5.7",
    "@types/lodash.throttle": "^4.1.8",
    "@types/react": "~18.2.33",
    "@types/react-test-renderer": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "benchmark": "^2.1.4",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "fast-check": "^3.13.2",
    "jest": "^29.7.0",
    "jest-expo": "^49.0.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.2.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsutils": "^3.21.0",
    "typescript": "^5.2.2"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/preset-env",
        "@types/node",
        "expo-modules-autolinking"
      ]
    }
  }
}
