{
  "name": "teams-generator",
  "private": true,
  "version": "1.3.3",
  "scripts": {
    "start": "cross-env APP_VARIANT=development expo start",
    "android": "cross-env APP_VARIANT=development expo run:android",
    "ios": "cross-env APP_VARIANT=development expo run:ios",
    "web": "cross-env APP_VARIANT=development expo start --web",
    "experiment": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/experiment.ts",
    "test": "vitest",
    "test:ci": "vitest --run",
    "coverage": "vitest --run --coverage related",
    "coverage:all": "vitest --run --coverage",
    "lint": "eslint . --config .eslintrc.production.js",
    "lint:fix": "eslint . --fix --config .eslintrc.production.js",
    "prepare": "husky install .husky",
    "eas-build-pre-install": "npm i -g pnpm@8.12.0",
    "build:development:android": "eas build --platform android --profile development",
    "build:development:ios": "eas build --platform ios --profile development",
    "build:development:ios:simulator": "eas build --platform ios --profile development-simulator",
    "build:preview": "eas build --platform android --profile preview",
    "build:sponsor": "eas build --platform android --profile sponsor",
    "build:staging": "eas build --platform android --profile staging",
    "build:production": "eas build --platform android --profile production --auto-submit",
    "update:preview": "cross-env APP_VARIANT=preview eas update --auto --platform android --channel preview",
    "update:sponsor": "cross-env APP_VARIANT=sponsor eas update --auto --platform android --channel sponsor",
    "update:staging": "cross-env APP_VARIANT=staging eas update --auto --platform android --channel staging",
    "update:production": "cross-env APP_VARIANT=production eas update --auto --platform android --channel production"
  },
  "dependencies": {
    "@effect/schema": "^0.52.0",
    "@expo/vector-icons": "13.0.0",
    "@fp-ts/optic": "^0.19.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@react-native-async-storage/async-storage": "~1.18.2",
    "@react-navigation/elements": "^1.3.19",
    "@react-navigation/native": "^6.1.8",
    "cross-env": "^7.0.3",
    "effect": "2.0.0-next.59",
    "expo": "^49.0.21",
    "expo-application": "~5.3.0",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.12",
    "expo-device": "~5.4.0",
    "expo-document-picker": "~11.5.4",
    "expo-file-system": "~15.4.5",
    "expo-linking": "~5.0.2",
    "expo-localization": "~14.3.0",
    "expo-sharing": "~11.5.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-updates": "~0.18.16",
    "fast-text-encoding": "^1.0.6",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@changesets/cli": "^2.27.1",
    "@cspell/dict-pt-br": "^2.3.0",
    "@expo/config-types": "49.0.0",
    "@types/benchmark": "^2.1.5",
    "@types/lodash.throttle": "^4.1.9",
    "@types/react": "~18.2.43",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitest/coverage-v8": "^1.0.4",
    "benchmark": "^2.1.4",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "fast-check": "^3.14.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "react-test-renderer": "18.2.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsutils": "^3.21.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vitest": "^1.0.4"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/preset-env",
        "@types/node",
        "expo-modules-autolinking"
      ]
    }
  }
}
