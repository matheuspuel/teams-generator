{
  "name": "teams-generator",
  "private": true,
  "version": "1.5.4",
  "scripts": {
    "start": "cross-env APP_VARIANT=development expo start",
    "android": "cross-env APP_VARIANT=development expo run:android",
    "ios": "cross-env APP_VARIANT=development expo run:ios",
    "web": "cross-env APP_VARIANT=development expo start --web",
    "lint": "eslint . --config .eslintrc.production.js",
    "lint:fix": "eslint . --fix --config .eslintrc.production.js",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "test:ci": "vitest --run",
    "coverage": "vitest --run --coverage related",
    "coverage:all": "vitest --run --coverage",
    "build:development:android": "eas build --platform android --profile development",
    "build:development:ios": "eas build --platform ios --profile development",
    "build:development:ios:simulator": "eas build --platform ios --profile development-simulator",
    "build:preview:android": "eas build --platform android --profile preview",
    "build:staging:android": "eas build --platform android --profile staging",
    "build:production:android": "eas build --platform android --profile production --auto-submit",
    "build:production:ios": "eas build --platform ios --profile production --auto-submit",
    "build:production": "eas build --profile production --auto-submit",
    "update:preview": "cross-env APP_VARIANT=preview eas update --auto --platform android --channel preview",
    "update:staging": "cross-env APP_VARIANT=staging eas update --auto --platform android --channel staging",
    "update:production": "cross-env APP_VARIANT=production eas update --auto --platform android --channel production"
  },
  "dependencies": {
    "@effect/typeclass": "^0.36.0",
    "@expo/vector-icons": "14.1.0",
    "@fp-ts/optic": "^0.25.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@react-native-async-storage/async-storage": "~2.1.2",
    "@react-navigation/elements": "^1.3.29",
    "@react-navigation/native": "^6.1.16",
    "cross-env": "^7.0.3",
    "effect": "3.17.7",
    "expo": "^53.0.22",
    "expo-application": "~6.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-document-picker": "~13.1.6",
    "expo-file-system": "~18.1.11",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "~0.28.17",
    "fast-text-encoding": "^1.0.6",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-google-mobile-ads": "^14.2.3",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "use-sync-external-store": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/runtime": "^7.28.2",
    "@changesets/cli": "^2.27.7",
    "@cspell/dict-pt-br": "^2.3.0",
    "@expo/config-types": "51.0.2",
    "@types/benchmark": "^2.1.5",
    "@types/lodash.throttle": "^4.1.9",
    "@types/react": "~19.0.14",
    "@types/use-sync-external-store": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "benchmark": "^2.1.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "fast-check": "^4.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsutils": "^3.21.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "listUnknownPackages": true,
        "exclude": [
          "@effect/schema",
          "@effect/typeclass",
          "@fp-ts/optic",
          "@paralleldrive/cuid2",
          "cross-env",
          "effect",
          "fast-text-encoding"
        ]
      }
    }
  }
}
