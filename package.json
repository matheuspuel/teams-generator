{
  "name": "sorteio-times",
  "private": true,
  "version": "1.0.1",
  "scripts": {
    "experiment": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/experiment.ts",
    "start": "SET APP_VARIANT=development&& expo start",
    "android": "SET APP_VARIANT=development&& expo run:android",
    "ios": "SET APP_VARIANT=development&& expo run:ios",
    "web": "SET APP_VARIANT=development&& expo start --web",
    "react-devtools": "react-devtools",
    "lint": "eslint .",
    "preview": "SET APP_VARIANT=preview&& expo-cli publish --release-channel default",
    "staging": "SET APP_VARIANT=staging&& expo-cli publish --release-channel staging",
    "postversion": "SET APP_VARIANT=production&& expo-cli publish --release-channel production"
  },
  "dependencies": {
    "@effect/data": "0.11.8",
